{
  "name": "agenttur",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Self-hosted AI agent with WhatsApp and TUI channels",
  "main": "dist/index.js",
  "bin": {
    "turion": "dist/cli/turion.js"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "setup": "ts-node-dev --respawn --transpile-only src/cli/turion.ts setup",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint \"src/**/*.ts\""
  },
  "dependencies": {
    "@whiskeysockets/baileys": "^7.0.0",
    "dotenv": "^16.4.5",
    "pino": "^9.4.0",
    "qrcode-terminal": "^0.12.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
